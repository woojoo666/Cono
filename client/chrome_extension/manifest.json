{
	"name": "Cono",
	"version": "0.0.1",
	"manifest_version": 2,

	"description": "Tagging Framework",
	"icons": {
	},

	"background": {
		"persistent": false,
		"scripts": ["background.js"]
	},
	"content_scripts": [{
			"matches": ["<all_urls>"],
			"js": ["tippy.all.min.js", "cono-inject.js", "cono-tooltip.js"],
			"css": ["cono-inject.css", "cono-tooltip.css"]
	}],
	"permissions": ["sessions","tabs","storage"],
    "web_accessible_resources": ["*"]
}
